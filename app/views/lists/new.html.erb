<div class="container">

<div class=addnewlist>
  <h1>Add a new list</h1>
</div>
    <%= simple_form_for @list do |f| %>
       <%= f.input :name %>
       <%= f.input :photo, as: :file %>
       <%= f.submit class: 'btn' %>

    <%= link_to 'Back to lists', lists_path, class: 'btn' %>

<% end %>
</div>
